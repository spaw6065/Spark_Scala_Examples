

object scalaclosureObj {
  val maxLength = 10
  def getlistmaxlengthelement(iList:List[String]) = {
     iList.foreach(x=> if (x.length > maxLength) println(x))    
    }
  
  def main(args:Array[String])={
    val sList = List("sumit","apple","potato","jist","summary","mesmerising","mesmerising")
  
    println("maximum length string in the list "+getlistmaxlengthelement(sList))
    
  }
}